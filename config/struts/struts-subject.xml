<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="subject" namespace="/" extends="common">
		<action name="subjectAction_*" class="com.san.action.SubjectAction" method="{1}">
			<!-- 显示成绩排名 -->
			<result name="showGradeRank">/ordinary.jsp</result>
			<!-- 没有题目 -->
			<result name="notSubject" type="redirectAction">subjectAction_showGradeRank.action</result>
			<!-- 显示题目 -->
			<result name="display" type="redirect">/ordinary.jsp</result>
			<!-- 显示下一题 -->
			<result name="displayNext" type="redirect">/ordinary.jsp</result>
			<!-- 显示上一题 -->
			<result name="displayLast">/ordinary.jsp</result>
		</action>
	</package>
</struts>
